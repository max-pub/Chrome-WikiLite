{
  "manifest_version": 2,
  "name": "WikiLite",
  "description": "removes the clutter from Wikipedia",
  "version": "1.1",
  "author": "www.max.pub",
  "icons": {
    "256": "bin/WikiLite.256.png"
  },
  "content_scripts": [{
    "matches": ["https://*.wikipedia.org/wiki/*"],
    "css": ["style.css"],
    "js": ["script.js"]
  }],
  "web_accessible_resources": [
    "flags/*.png"
  ]
}